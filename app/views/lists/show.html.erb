<h1>List</h1>
<%= @list.name %>

<%= link_to "add a movie", new_list_bookmark_path(@list), class:"btn btn-primary"%>
<ul>
  <% @list.movies.each do |movie| %>
  <% bookmark = Bookmark.find_by(movie: movie, list: @list) %>
  <li>
    <%= movie.title %> <%= link_to "delete", bookmark_path(bookmark), method: :delete, data: { confirm: "Are you sure?" }, class: "text-danger" %>
  </li>


  <% end %>
</ul>
